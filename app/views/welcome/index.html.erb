 <% if @categories_evento.first != nil %>
<!-- si HAY imagenes cargadas en las publicaciones de abajo -->  
  <div class="container-fluid ">
    <h1>Eventos</h1>
    <hr/>
    <div class="row">
      <div class="col-md-12">
        <div class="carousel carousel-showmanymoveone slide" id="carousel-tilenav" data-interval="false">
          <div class="carousel-inner">
            <div class="item active">
                   <% @categories_evento.first(3).each do |e|%>
                      <div class="col-sm-4">
                      <% if user_signed_in? %>
                         <% @mylink = "/categories/" + e.id.to_s %>
                       <% else %>
                         <% @mylink = "/categories1/" + e.id.to_s %>
                       <% end %>
                          <a rel="nofollow" rel="noreferrer" href="<%= @mylink%>" >
                          <%= image_tag(e.avatar1.url, :class => "img-responsive") %>
                          </a>
                      </div>
                    <% end %>  
                    
              </div><!-- cierre de item-active -->

            <% for i in 3..@categories_evento.length-1 do %>
              <% if i % 3 == 0  %>
              <div class="item">
              <%= "i:#{i}" %>
              <% @aux = i %>
              <% @aux2 = @aux + 2 %>

              <% if @aux2 > @categories_evento.length-1 %>
                <% @aux2 = @categories_evento.length %>
              <% end %>  
              <%= "aux2:#{@aux2}, aux:#{@aux}" %>
                <% @categories_evento[@aux..@aux2].each do |e|%>    
                  <div class="col-sm-4">
                  <% if user_signed_in? %>
                         <% @mylink = "/categories/" + e.id.to_s %>
                       <% else %>
                         <% @mylink = "/categories1/" + e.id.to_s %>
                       <% end %>
                      <a rel="nofollow" rel="noreferrer" href="<%= @mylink%>" >
                        <%= image_tag(e.avatar1.url, :class => "img-responsive") %>
                      </a>
                  </div>
                <% end %> 

                   
              </div>
            <% end %>   
            <% end %>  


        </div><!-- cierre de carousel-inner -->



         <a rel="nofollow" rel="noreferrer"class="left carousel-control" href="#carousel-tilenav" data-slide="prev"><i class="glyphicon glyphicon-chevron-left"></i></a>
         <a rel="nofollow" rel="noreferrer"class="right carousel-control" href="#carousel-tilenav" data-slide="next"><i class="glyphicon glyphicon-chevron-right"></i></a>
          </div><!-- cierre de carousel -->
        </div><!-- cierre de col -->
